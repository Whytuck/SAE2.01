<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Lot7.CtrlStat">
   <children>
      <Label text="Statistiques des Spectacles">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <TableView fx:id="statistiquesTable" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="102.0" />
          <TableColumn prefWidth="151.0" />
            <TableColumn prefWidth="108.0" />
            <TableColumn prefWidth="233.0" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="BOTTOM_RIGHT" prefHeight="46.0" prefWidth="550.0" spacing="10.0">
         <children>
            <Button fx:id="refreshButton" mnemonicParsing="false" onAction="#rafraichirStat" text="Rafraichir" />
            <Button fx:id="annulerButton" mnemonicParsing="false" onAction="#annulerStat" text="Annuler" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
   </padding>
